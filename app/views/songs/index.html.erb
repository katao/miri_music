<p id="notice"><%= notice %></p>

<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>Gn</th>
      <th>Album</th>
      <th>Artist</th>
      <th>Title</th>
      <th>Track num</th>
      <th>Mood</th>
      <th>Tempo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.gn_id %></td>
        <td><%= song.album_id %></td>
        <td><%= song.artist %></td>
        <td><%= song.title %></td>
        <td><%= song.track_num %></td>
        <td><%= song.mood %></td>
        <td><%= song.tempo %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
